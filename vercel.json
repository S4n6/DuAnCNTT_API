{
  "version": 2,
  "builds": [
    {
      "src": "apps/auth/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/user/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/event/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/registration/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/feedback-and-rating/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/notification/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/participants/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/schedule/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "apps/check-in-out/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "apps/auth/$1"
    },
    {
      "src": "/user/(.*)",
      "dest": "apps/user/$1"
    },
    {
      "src": "/event/(.*)",
      "dest": "apps/event/$1"
    },
    {
      "src": "/registration/(.*)",
      "dest": "apps/registration/$1"
    },
    {
      "src": "/feedback-and-rating/(.*)",
      "dest": "apps/feedback-and-rating/$1"
    },
    {
      "src": "/notification/(.*)",
      "dest": "apps/notification/$1"
    },
    {
      "src": "/participants/(.*)",
      "dest": "apps/participants/$1"
    },
    {
      "src": "/schedule/(.*)",
      "dest": "apps/schedule/$1"
    },
    {
      "src": "/check-in-out/(.*)",
      "dest": "apps/check-in-out/$1"
    }
  ]
}
